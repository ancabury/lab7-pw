- if @winner
  %h1= @winner
- if @board
  %tr
    %td.center.cell-hover
      = @board[0][0]
    %td.center.cell-hover
      = @board[0][1]
    %td.center.cell-hover
      = @board[0][2]
  %tr
    %td.center.cell-hover
      = @board[1][0]
    %td.center.cell-hover
      = @board[1][1]
    %td.center.cell-hover
      = @board[1][2]
  %tr
    %td.center.cell-hover
      = @board[2][0]
    %td.center.cell-hover
      = @board[2][1]
    %td.center.cell-hover
      = @board[2][2]
